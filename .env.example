 # Database Configuration
 # DATABASE_URL: use Supabase pooled TRANSACTION URL in production runtime (port 6543)
 # DIRECT_URL: use direct DB URL for migrations only
DATABASE_URL="your_database_url_here"
DIRECT_URL="your_direct_url_here"
 # On Vercel serverless, keep this low to avoid connection spikes per instance
PG_POOL_MAX="2"
PG_POOL_IDLE_TIMEOUT_MS="10000"
PG_POOL_CONNECT_TIMEOUT_MS="15000"
 
